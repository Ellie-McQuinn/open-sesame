{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "${mod_id}",
    "version": "${version}",
    "metadata": {
      "name": "${mod_name}",
      "description": "${description}",
      "contributors": {

      },
      "contact": {
        "homepage": "${homepage}",
        "issues": "${issue_tracker}",
        "sources": "${sources_url}"
      },
      "license": "${license}",
      "icon": "${mod_id}.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=${quilt_loader_version}"
      },
      {
        "id": "fabric-api",
        "versions": ">=${fabric_api_version}"
      },
      {
        "id": "minecraft",
        "versions": "${fl_minecraft_constraint}"
      },
      {
        "id": "java",
        "versions": ">=${java_version}"
      }
    ]
  },
  "mixin": [
    "${mod_id}.mixins.json"
  ]
}
